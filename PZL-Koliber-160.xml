 <system file="fcs.xml"/>

 <flight_control name="FCS: PZL-Koliber-160">

  <channel name="Landing Gear">
   <switch name="fcs/gear-no-wow">
     <default value="1"/>
     <test logic="AND" value="0">
         gear/unit[1]/WOW eq 1
         gear/unit[2]/WOW eq 1
     </test>
   </switch>
  </channel>

  <channel name="Flaps">
   <kinematic name="Flaps Control">
    <input>fcs/flap-cmd-norm</input>
    <traverse>
      <setting>
         <position> 15 </position>   <!-- the 950kg MTOW version have always flaps -->
         <time>      0 </time>
      </setting>
      <setting>
         <position> 30 </position>
         <time>      5 </time>
      </setting>
    </traverse>
    <output>fcs/flap-pos-deg</output>
   </kinematic>

   <aerosurface_scale name="Flap Normalization">
    <input>fcs/flap-pos-deg</input>
    <domain>
      <min>  15 </min>
      <max> 30 </max>
    </domain>
    <range>
      <min> 0 </min>
      <max> 1 </max>
    </range>
    <output>fcs/flap-pos-norm</output>
   </aerosurface_scale>
  </channel>

 </flight_control>


