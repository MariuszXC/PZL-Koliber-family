<?xml version="1.0" encoding="UTF-8"?>
<!-- Na podstawie piper-archer-sound -->
<PropertyList>

  <fx>
    <engine>
      <name>engine</name>
      <mode>looped</mode>
      <path>Sounds/wasp.wav</path>
      <property>engines/engine[0]/running</property>
      <position>
        <x>-3.0</x>
        <y> 0.0</y>
        <z> 0.0</z>
      </position>
      <pitch>
	<property>engines/engine[0]/rpm</property>
	<factor>0.0004</factor>
	<min>0.3</min>
	<max>1000.0</max>
	<offset>0.1</offset>
      </pitch>
      <volume>
	<property>sim/current-view/internal</property>
	<factor>-0.25</factor>
	<offset>0.8</offset>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>10000.0</max-dist>
    </engine>

    <crank>
      <name>cough</name>
      <path>Sounds/coughing.wav</path>
      <property>/engines/engine/running</property>
      <position>
        <x>-3.0</x>
        <y>0.0</y>
        <z>0.0</z>
      </position>
      <volume>
        <offset>0.3</offset>
      </volume>
    </crank>

    <engine>
      <name>cranking</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <position>
        <x>-3.0</x>
        <y> 0.0</y>
        <z> 0.0</z>
      </position>
      <condition>
        <and>
          <property>engines/engine[0]/cranking</property>
        </and>
      </condition>
      <volume>
	<property>sim/current-view/internal</property>
	<factor>-0.2</factor>
	<offset>0.9</offset>
      </volume>
      <pitch>
        <property>engines/engine/rpm</property>
        <factor>0.002</factor>
        <min>0.2</min>
        <max>1.2</max>
        <offset>0.04</offset>
      </pitch>
      <reference-dist>10.0</reference-dist>
      <max-dist>10000.0</max-dist>
    </engine>

 <propeller>
      <name>propeller</name>
      <mode>looped</mode>
      <path>Sounds/wasp.wav</path>
      <position>
        <x>-3.0</x>
        <y> 0.0</y>
        <z> 0.0</z>
      </position>
      <condition>
        <greater-than>
          <property>engines/engine/rpm</property>
          <value>10</value>
        </greater-than>
      </condition>
      <volume>
        <property>engines/engine/rpm</property>
        <factor>0.0001</factor>
        <max>0.25</max>
      </volume>
      <pitch>
        <property>engines/engine/rpm</property>
        <factor>0.0004</factor>
        <min>0.0</min>
        <max>2.0</max>
        <offset>0.01</offset>
      </pitch>
    </propeller>

<wind>
	<name>wind</name>
	<mode>looped</mode>
	<path>Sounds/wind.wav</path>
	<volume>
		<property>velocities/airspeed-kt</property>
		<factor>0.0005</factor>
		<min>0.01</min>
		<max>0.4</max>
	</volume>
</wind>
<!-- touchdown to lÄ…dowanie -->
        <rumble>
            <name>rumble</name>
            <mode>looped</mode>
            <path>Sounds/rumble.wav</path>
            <condition>
                <or>
                    <property>gear/gear[0]/wow</property>
                    <property>gear/gear[1]/wow</property>
                    <property>gear/gear[2]/wow</property>
                </or>
                <!-- <property>sim/current-view/internal</property> -->
            </condition>
            <position>
                <x>1.0</x>
                <y>0.0</y>
                <z>-1.5</z>
            </position>
            <volume>
                <property>velocities/groundspeed-kt</property>
                <type>log</type>
                <factor>0.25</factor>
            </volume>
        </rumble>


      <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <or>
          <property>gear/gear[0]/wow</property>
          <property>gear/gear[1]/wow</property>
          <property>gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>velocities/speed-down-fps</property>
        <factor>0.5</factor>
        <max>1.0</max>
      </volume>
      <volume>
        <property>velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <max>0.2</max>
        <max>1.0</max>
      </volume>
      <volume>
        <internal>dt_stop</internal>
        <factor>0.05</factor>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>velocities/airspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.2</offset>
      </pitch>
    </squeal>
         <flap>
            <name>flaps</name>
            <mode>in-transit</mode>
            <path>Sounds/flaps.wav</path>
            <property>surface-positions/flap-pos-norm</property>
            <position>
                <x>0.5</x>
                <y>0.0</y>
                <z>0.5</z>
            </position>
            <volume>
                <factor>0.8</factor>
            </volume>
            <reference-dist>10.0</reference-dist>
            <max-dist>20.0</max-dist>
        </flap>


     <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>Sounds/stall.wav</path>
      <volume>
        <property>instrumentation/stall-warn/sound-volume-norm</property>
      	 <offset>-1</offset>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>50.0</max-dist>
    </stall>

<pump>
	<name>FuelPump</name>
	<type>avionics</type>
	<mode>looped</mode>
	<path>Aircraft/PZL-Koliber-160A/Sounds/FuelPump.wav</path>
	<condition>
		<and>
		<property>/controls/switches/battery</property>
		<property>/controls/switches/fuelpump</property>
		</and>
	</condition>
	<volume>
		<factor>0.5</factor>
	</volume>
</pump>

<gyro>
	<name>Gyro</name>
	<type>avionics</type>
	<mode>looped</mode>
	<path>Aircraft/PZL-Koliber-160A/Sounds/gyro.wav</path>
	<condition>
		<and>
		<property>/controls/switches/battery</property>
		<property>/controls/switches/turn-coordinator</property>
		</and>
		<volume>
			<factor>1.0</factor>
		</volume>
	</condition>
</gyro>

    <!--                  Toggle Switches                  -->
<click>
  <name>Battery on</name>
  <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonPower.wav</path>
  <condition>
	<property>/controls/switches/battery</property>
  </condition>
  <volume>
	<factor>1.0</factor>
  </volume>
</click>

<click>
  <name>Battery off</name>
  <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonPower.wav</path>
  <condition>
	<not>
	  <property>controls/switches/battery</property>
	</not>
  </condition>
  <volume>
	<factor>1.0</factor>
  </volume>
</click>

<click>
  <name>Generator on</name>
  <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonPower.wav</path>
  <condition>
	<property>controls/switches/alternator</property>
  </condition>
  <volume>
	<factor>1.0</factor>
  </volume>
</click>

<click>
  <name>Generator off</name>
  <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonPower.wav</path>
  <condition>
	<not>
	  <property>controls/switches/alternator</property>
	</not>
  </condition>
  <volume>
	<factor>1.0</factor>
  </volume>
</click>

<click>
  <name>Fuel Pump on</name>
  <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
  <condition>
	<property>/controls/switches/fuelpump</property>
  </condition>
  <volume>
	<factor>1.0</factor>
  </volume>
</click>

<click>
  <name>Fuel Pump off</name>
  <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
  <condition>
	<not>
	  <property>/controls/switches/fuelpump</property>
	</not>
  </condition>
  <volume>
	<factor>1.0</factor>
  </volume>
</click>

<click>
  <name>Nav Light On</name>
  <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
  <condition>
	<property>controls/lighting/nav-lights</property>
  </condition>
  <volume>
	<factor>1.0</factor>
  </volume>
</click>

    <click>
      <name>Nav Light Off</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>controls/lighting/nav-lights</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Landing Light On</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <property>/controls/lighting/landing-lights</property>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Landing Light Off</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>/controls/lighting/landing-lights</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Turn and Bank On</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <property>/controls/switches/turn-coordinator</property>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Turn and Bank Off</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>/controls/switches/turn-coordinator</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Flashing Beacon On</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <property>controls/switches/beacon</property>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Flashing Beacon Off</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>controls/switches/beacon</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Pitot Heat On</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <property>controls/anti-ice/pitot-heat</property>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Pitot Heat Off</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>controls/anti-ice/pitot-heat</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

   <click>
      <name>Magnetos-off</name>
      <path>Sounds/click.wav</path>
      <condition>
        <not>
          <property>controls/switches</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Magnetos-R</name>
      <path>Sounds/click.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine/magnetos</property>
          <value>1</value>
        </equals>
      </condition>
      <volume>
        <factor>0.8</factor>
      </volume>
      <reference-dist>0.5</reference-dist>
      <max-dist>5.0</max-dist>
    </click>

    <click>
      <name>Magnetos-L</name>
      <path>Sounds/click.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine/magnetos</property>
          <value>2</value>
        </equals>
      </condition>
      <volume>
        <factor>0.8</factor>
      </volume>
      <reference-dist>0.5</reference-dist>
      <max-dist>5.0</max-dist>
    </click>

    <click>
      <name>Magnetos-B</name>
      <path>Sounds/click.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine/magnetos</property>
          <value>3</value>
        </equals>
      </condition>
      <volume>
        <factor>0.8</factor>
      </volume>
      <reference-dist>0.5</reference-dist>
      <max-dist>5.0</max-dist>
    </click>

     <click>
      <name>Taxi Light On</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <property>controls/lighting/taxi-lights</property>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Taxi Light Off</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/ButtonLights.wav</path>
      <condition>
        <not>
          <property>controls/lighting/taxi-lights</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <squish>
      <name>Primer Out</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <property>controls/engines/engine/primer-pump</property>
      </condition>
      <volume>
        <factor>0.1</factor>
      </volume>
    </squish>

    <squish>
      <name>Primer In</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <not>
          <property>controls/engines/engine/primer-pump</property>
        </not>
      </condition>
      <volume>
        <factor>0.1</factor>
      </volume>
    </squish>
    <!-- zawÃ³r paliwa -->
    <click>
      <name>Fuel valve</name>
      <path>Aircraft/PZL-Koliber-160A/Sounds/pump-valve.wav</path>
      <condition>
        <property>controls/fuel-selector</property>
      </condition>
      <volume>
        <factor>0.5</factor>
      </volume>
    </click>

  </fx>

</PropertyList>
