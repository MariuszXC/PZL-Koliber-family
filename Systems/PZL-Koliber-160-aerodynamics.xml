				<property>metrics/cbarw-ft</property>
				<table>
					<independentVar lookup="row">aero/alpha-deg</independentVar>
					<tableData>
						-15.0	0.111494
						-10.0	0.075040
						-5.0	0.018288
						0.0	-0.071309
						5.0	-0.399068
						10.0	-2.241494
						15.0	-0.837137
					</tableData>
				</table>
				<!-- hack to give less pitch moment in a strong sideslip -->
				<table>
					<independentVar lookup="row">aero/beta-deg</independentVar>
