<aerodynamics>
	<function name="aero/alpha-wing-deg">
		<todegrees>
			<property>aero/alpha-wing-rad</property>
		</todegrees>
	</function>
	<function name="/orientation/alpha-wing-deg">
		<todegrees>
			<property>aero/alpha-wing-rad</property>
