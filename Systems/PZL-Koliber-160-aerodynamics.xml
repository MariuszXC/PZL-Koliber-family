	<axis name="DRAG">
		<function name="aero/force/Drag_base">
			 <description>Minimum, induced and sideslip drag</description>
			 <product>
				<property>aero/qbar-psf</property>
				<property>metrics/Sw-sqft</property>
				<sum>
					<table>
						<independentVar lookup="row">aero/alpha-deg</independentVar>
						<independentVar lookup="column">aero/beta-deg</independentVar>
						<tableData>
									-30.0		-15.0		0.0		15.0		30.0
							-15.0	0.289067	0.257782	0.054462	0.257782	0.289067
							-10.0	0.348402	0.261072	0.026672	0.261072	0.348402
							-5.0	0.521274	0.266642	0.017418	0.266642	0.521274
							0.0		0.354093	0.235348	0.028079	0.235348	0.354093
							5.0		0.495578	0.227590	0.059030	0.227590	0.495578
							10.0	0.570819	0.386695	0.108217	0.386695	0.570819
							15.0	0.572505	0.304922	0.219524	0.304922	0.572505
						</tableData>
					</table>
					<value>0.01128</value> <!-- correction value -->
				</sum>
			</product>
		</function>
		
