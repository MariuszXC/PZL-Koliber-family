<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<filter>
			<name>RPM lowpass</name>
			<debug>false</debug>
			<type>exponential</type>
			<filter-time>0.1</filter-time>

			<input>
				<property>engines/engine[0]/rpm</property>
			</input>

			<output>/gauges/RPM-pos</output>
	</filter>
</PropertyList>
