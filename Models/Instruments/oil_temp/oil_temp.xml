<?xml version="1.0" encoding="UTF-8"?>


<PropertyList>
	<path>oil_temp.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>wskazowka</object-name>
                <object-name>wskazowka-off</object-name>
		<object-name>tarcza</object-name>
  <emission>
    <red>0.8</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>/systems/electrical/outputs/panel-norm</factor-prop>
  </emission>
	</animation>
        
<animation>
  <type>select</type>
  <object-name>wskazowka</object-name>
  <condition>
    <property>systems/electrical/outputs/bus-dc</property>
  </condition>
</animation>
<animation>
  <type>select</type>
  <object-name>wskazowka-off</object-name>
  <condition>
    <not><property>systems/electrical/outputs/bus-dc</property></not>
  </condition>
</animation>
	
<animation>
  <type>rotate</type>
  <object-name>wskazowka</object-name>
  <property>engines/engine[0]/oil-temperature-degf</property>
  <interpolation>
   <entry><ind>100</ind><dep>0.00</dep></entry>
   <entry><ind>180</ind><dep>49.64</dep></entry>
   <entry><ind>260</ind><dep>95.06</dep></entry>
  </interpolation>
  <center>
   <x-m>-0.00178</x-m>
   <y-m>0.00</y-m>
   <z-m>-0.00926</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>
