<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

 <path>vsi-1.1.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle</object-name>
   <emission>
    <red>0.8</red>
    <green>0.4</green>
    <blue>0.2</blue>
